{
  "name": "naive-ui-nuxt",
  "version": "1.0.5",
  "description": "Naive UI module for Nuxt",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "packageManager": "pnpm@7.13.2",
  "keywords": [
    "naive-ui",
    "nuxt"
  ],
  "homepage": "https://github.com/kuizuo/naive-ui-nuxt#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuizuo/naive-ui-nuxt.git"
  },
  "bugs": {
    "url": "https://github.com/kuizuo/naive-ui-nuxt/issues"
  },
  "scripts": {
    "build": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@nuxt/kit": "^3.2.2"
  },
  "devDependencies": {
    "@css-render/vue3-ssr": "^0.15.12",
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.2.2",
    "@nuxt/test-utils": "^3.2.2",
    "eslint": "^8.34.0",
    "naive-ui": "^2.34.3",
    "nuxt": "^3.2.2"
  },
  "peerDependencies": {
    "naive-ui": ">=2.29.0"
  }
}
